*clase libro
package pkg11_graciagarcia;

import javax.swing.JOptionPane;

public class libro {
        //atributos
    String lee;
    String canta;
    
    public void habla(){
        String habla = null;
        System.out.println("el libro"+habla+"en el cuarto"); 
    }
    
    public void corre(){
        System.out.println("corre");
    }

    public void pregunta(){
    
        String habla = JOptionPane.showInputDialog(null, "cuantohabla el libro");
        String corre = JOptionPane.showInputDialog(null, "Ingrese cuanto corre");
    }
}

*clase estudiante
package pkg11_graciagarcia;

import javax.swing.JOptionPane;

public class estudiante {
        //atributos
    String corre;
    String salta;
    
    public void niño(){
        System.out.println("el perro"+corre+"en el pasto"); 
    }
    
    public void niña(){
        System.out.println("salta");
    }

    public void pregunta(){
    
        corre = JOptionPane.showInputDialog(null, "cuantos kilometros corre");
        String saltar = JOptionPane.showInputDialog(null, "Ingrese cuanto salta");
    }
}

*clase cuenta bancaria
package pkg11_graciagarcia;

import java.util.Scanner;


public class cuentabancaria {
     //atributos
    int a;
    int b;
    int total;
    
    // metodos 
    public void sumar() {

        total = a + b;
        System.out.println(total);
    }
    
    public void multiplicar() {

        total= a * b;
        System.out.println(total);
    }
    
    public void ingresardatos(){
        
        Scanner scanner = new Scanner(System.in);
        System.out.print("Ingrese el valor (numero): ");
        b = scanner.close();
        System.out.print("Ingrese el valor (numero): ");
        a = scanner.nextInt();
        scanner.close();
        }
}

*clase auto
package pkg11_graciagarcia;

import java.util.Scanner;

public class auto {
     //atributos
    int a ;
    int b;
    int total;
    
    // metodos 
    public void sumarkilometros() {

        total = a + b;
        System.out.println(total);
    }
    
    public void restarkilomtroenuso() {

        total= a * b;
        System.out.println(total);
    }
    
    public void ingresardatos(){
        
        Scanner scanner = new Scanner(System.in);
        System.out.print("Ingrese el valor (numero): ");
        b = scanner.next();
        System.out.print("Ingrese el valor (numero): ");
        a = scanner.nextInt();
        scanner.close();
        }
}
